{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^20.18.2",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "form-data": "^4.0.5",
    "lucide-react": "^0.476.0",
    "multer": "^1.4.5-lts.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.9.1",
    "react-scripts": "5.0.1",
    "sharp": "^0.33.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "dev:full": "concurrently \"npm run server\" \"npm start\" \"npm run ml:server\" \"npm run label-studio\"",
    "build:ios": "cd ios && xcodebuild -project HeartsCardCapture.xcodeproj -scheme HeartsCardCapture -configuration Release -derivedDataPath ./build -destination generic/platform=iOS clean build",
    "ml:install": "cd ml && pip install -r requirements.txt",
    "ml:server": "cd ml/server && python inference_server.py",
    "ml:train": "cd ml/scripts && python train.py",
    "ml:prepare": "cd ml/scripts && python prepare_dataset.py",
    "ml:verify-labels": "cd ml/scripts && python verify_labels.py",
    "label-studio": "python -c \"import sys; sys.argv = ['label-studio', 'start', '--port', '8080', '--no-browser']; from label_studio.server import main; main()\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "fs": false
  },
  "devDependencies": {
    "autoprefixer": "^10.4.24",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19"
  }
}
